<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="历史记录         "
            android:textSize="11.5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="请求总次数：   "
            android:textSize="11.5dp" />

        <TextView
            android:id="@+id/all_n"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="MI8 SE"
            android:textColor="@color/colorAccent"
            android:textSize="11.5dp"
            android:textStyle="normal" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="成功次数：   "
            android:textSize="11.5dp" />

        <TextView
            android:id="@+id/success_n"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="XIAOMI"
            android:textColor="@color/colorAccent"
            android:textSize="11.5dp"
            android:textStyle="normal" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="历史记录         "
            android:textSize="11.5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="曝光总次数：   "
            android:textSize="11.5dp" />

        <TextView
            android:id="@+id/all_n_e"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="MI8 SE"
            android:textColor="@color/colorAccent"
            android:textSize="11.5dp"
            android:textStyle="normal" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="成功次数：   "
            android:textSize="11.5dp" />

        <TextView
            android:id="@+id/success_n_exx"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="XIAOMI"
            android:textColor="@color/colorAccent"
            android:textSize="11.5dp"
            android:textStyle="normal" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="历史记录         "
            android:textSize="11.5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="点击总次数：   "
            android:textSize="11.5dp" />

        <TextView
            android:id="@+id/all_n_c"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="MI8 SE"
            android:textColor="@color/colorAccent"
            android:textSize="11.5dp"
            android:textStyle="normal" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="成功次数：   "
            android:textSize="11.5dp" />

        <TextView
            android:id="@+id/success_n_c"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="XIAOMI"
            android:textColor="@color/colorAccent"
            android:textSize="11.5dp"
            android:textStyle="normal" />

    </LinearLayout>

</LinearLayout>